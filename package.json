{
  "name": "get-gradle-versions",
  "private": true,
  "description": "Expose latest Gradle versions for an action",
  "scripts": {
    "build": "ncc build src/main.ts --target es2022",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KyoriPowered/get-gradle-versions.git"
  },
  "keywords": [
    "gradle",
    "java",
    "actions",
    "github"
  ],
  "author": "KyoriPowered",
  "license": "GNU Lesser General Public License, version 3.0",
  "packageManager": "yarn@3.6.4",
  "devDependencies": {
    "@types/eslint": "^8.44.3",
    "@types/jest": "^29.5.5",
    "@types/node": "20.8.10",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "@vercel/ncc": "^0.38.0",
    "eslint": "^8.50.0",
    "eslint-plugin-jest": "^27.4.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@actions/core": "^1.10.1"
  }
}
