{
  "name": "get-gradle-versions",
  "private": true,
  "description": "Expose latest Gradle versions for an action",
  "scripts": {
    "build": "ncc build src/main.ts --target es2022",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KyoriPowered/get-gradle-versions.git"
  },
  "keywords": [
    "gradle",
    "java",
    "actions",
    "github"
  ],
  "author": "KyoriPowered",
  "license": "GNU Lesser General Public License, version 3.0",
  "packageManager": "yarn@4.1.0",
  "devDependencies": {
    "@types/eslint": "^8.44.8",
    "@types/jest": "^29.5.10",
    "@types/node": "20.12.3",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.55.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "@actions/core": "^1.10.1"
  }
}
