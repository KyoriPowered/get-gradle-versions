{
  "name": "get-gradle-versions",
  "private": true,
  "description": "Expose latest Gradle versions for an action",
  "scripts": {
    "build": "ncc build src/main.ts --target es2021",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KyoriPowered/get-gradle-versions.git"
  },
  "keywords": [
    "gradle",
    "java",
    "actions",
    "github"
  ],
  "author": "KyoriPowered",
  "license": "GNU Lesser General Public License, version 3.0",
  "packageManager": "yarn@3.4.1",
  "devDependencies": {
    "@types/eslint": "^8",
    "@types/jest": "^29.2.4",
    "@types/node": "16",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "@vercel/ncc": "^0.36.0",
    "eslint": "^8.30.0",
    "eslint-plugin-jest": "^27.1.7",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "prettier": "^2.8.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@actions/core": "^1.10.0"
  }
}
